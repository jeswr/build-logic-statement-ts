{
  "name": "build-logic-statement-ts",
  "version": "0.1.0",
  "description": "Build logical statements",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint lib/* --ext .ts",
    "lint-fix": "eslint lib/* --ext .ts --fix",
    "build": "tsc",
    "pre-publish": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeswr/build-logic-statement-ts.git"
  },
  "keywords": [
    "logic",
    "statement",
    "build",
    "typescript"
  ],
  "author": "Jesse Wright <jesse.wright@anu.edu.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeswr/build-logic-statement-ts/issues"
  },
  "homepage": "https://github.com/jeswr/build-logic-statement-ts#readme",
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "pre-commit": "^1.2.2",
    "simplify-logic-statement-ts": "github:jeswr/simplify-logic-statement-ts",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ]
}
